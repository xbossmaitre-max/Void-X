 ],

 onStart: async function() {}, // Empty function as this is an auto-timer command

 onLoad: async function({ api }) {
 setInterval(() => {
 const now = new Date(Date.now() + 25200000).toLocaleString('en-US', {
 hour12: false,
 hour: '2-digit',
 minute: '2-digit',
 second: '2-digit'
 }).split(',')[1].trim();
 
 const scheduledMessage = this.messages.find(item => item.timer === now);
 
 if (scheduledMessage) {
 global.GoatBot.allThreadID.forEach(threadID => {
 api.sendMessage(scheduledMessage.message, threadID);
 });
 }
 }, 1000);
 }
};
